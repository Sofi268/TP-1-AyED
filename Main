int main(){
    Fruta f;
    int CANTIDAD_ACTUAL=0; //Hay que acordarse de sumar cada vez que metemos fruta al cajon y restarle cuando vaciamos.
    f.setCantFruta();
    Cajon Cajones[10];
    for (int i=0; i<10; i++){
        Cajones[i].setID(i);
    }
    int k=1;
    int i;
    while (k>0&&Cajones[i].LLENO!=true){
        k= Cajones[i].agregarFrutas;
    }
    
}
